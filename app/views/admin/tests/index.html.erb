<div class="container">
  <h2><%= t('.header')%></h2>
  <p>
    <%= link_to t('.new_test'), new_admin_test_path, class: 'btn btn-primary' %>
  </p>
</div>
<%= render 'shared/errors', resource: @test %>
<table class="table table-striped table-hover">
  <tr>
    <th class="sort-by-title pointer">
      <%= t('label.test') %> <%= octicon 'arrow-up', class: 'text-success hide'
      %> <%= octicon 'arrow-down', class: 'text-success hide' %>
    </th>
    <th><%= t('label.level') %></th>
    <th><%= t('label.question') %></th>
    <th><%=t('.time')%></th>
    <th><%= t('actions.value') %></th>
  </tr>
  <%= render @tests %>
</table>
