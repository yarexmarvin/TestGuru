<div  class="container">
  <h2><%= t('.header')%></h2>
  <p>
    <%= link_to t('.new_test'), new_admin_test_path, class: 'btn btn-primary' %>
  </p>
</div>
<%= render 'shared/errors', resource: @test %>
<table class="table table-striped table-hover">
  <tr>
    <th class="sort-by-title pointer">
      <%= t('label.test') %>
      <%= octicon 'arrow-up', class: 'text-success hide' %>
      <%= octicon 'arrow-down', class: 'text-success hide' %>
    </td>
    <th><%= t('label.level') %></td>
    <th><%= t('label.question') %></td>
    <th><%= t('actions.value') %></td>
  </tr>
<%= render @tests %>
</table>
