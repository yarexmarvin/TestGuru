<h2><%= t('.header', title: @test.title) %></h2>
<hr />
<span class="display-5"><%= t('actions.test')%></span><br>
<div class="mt-3">
  <%= link_to t('.new_question'), new_admin_test_question_path(@test), class: 'btn btn-primary' %> 
  <%= link_to t('system.button.edit'), edit_admin_test_path(@test), class: 'btn btn-warning' %>
  <%= link_to t('system.button.back'), admin_tests_path, class: 'btn btn-secondary' %>
</div>
<p>
  <strong><%= t('.time', timer: @test.timer ) %></strong>
</p>
<hr />

<table class="table table-striped">
  <tr>
    <th><%= t('label.question')%></th>
    <th colspan="3"><%= t('actions.value')%></th>
  </tr>
  <%= render @test.questions %>
</table>
