<tr>
  <td><%= test.title %></td>
  <td><%= level_translation(test.level) %></td>
  <td><%= questions_amount(test) %></td>
  <td><%= button_to "#{test.completed?? t('.completed') : t('.start')}", start_test_path(test), 
                                    class: "btn #{test.completed? ? 'btn-disabled' :' btn-success'}",
                                    disabled: test.completed?%></td>
</tr>

